{
  "expo": {
    "name": "DataRecorder",
    "slug": "DataRecorderApp",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "githubUrl": "https://github.com/bilalcorbacioglu/DataRecorderApp",
